{
  "name": "cloudflare-saas-stack",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.2.21",
  "engines": {
    "bun": ">=1.2.21"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "deploy:full": "bun run lint && bun run db:migrate:prod && opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "deploy:staging": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env staging",
    "ci:build": "bun run lint && opennextjs-cloudflare build",
    "ci:deploy": "bun run db:migrate:prod && opennextjs-cloudflare deploy",
    "setup": "bunx tsx scripts/setup.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate:dev": "drizzle-kit migrate",
    "db:migrate:prod": "NODE_ENV=production drizzle-kit migrate",
    "db:studio:dev": "drizzle-kit studio",
    "db:studio:prod": "NODE_ENV=production drizzle-kit studio",
    "studio:start": "drizzle-kit studio > /dev/null 2>&1 & echo $! > .studio.pid && echo 'Drizzle Studio started at https://local.drizzle.studio'",
    "studio:stop": "if [ -f .studio.pid ]; then kill $(cat .studio.pid) && rm .studio.pid && echo 'Drizzle Studio stopped'; else echo 'Drizzle Studio is not running'; fi",
    "studio:restart": "bun run studio:stop && bun run studio:start",
    "db:query": "bunx wrangler d1 execute atenra-dev-db --local --command",
    "db:query:prod": "bunx wrangler d1 execute atenra-dev-db --command",
    "stripe:sync": "bun run scripts/sync-stripe-products.ts",
    "stripe:update-prices": "bun run scripts/update-stripe-prices.ts"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.10.0",
    "@clack/prompts": "^0.11.0",
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@iarna/toml": "^2.2.5",
    "@libsql/client": "^0.10.0",
    "@paypal/react-paypal-js": "^8.9.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tiptap/extension-placeholder": "^3.11.1",
    "@tiptap/pm": "^3.11.1",
    "@tiptap/react": "^3.11.1",
    "@tiptap/starter-kit": "^3.11.1",
    "@types/bcryptjs": "^3.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "lucide-react": "^0.438.0",
    "next": "^15.1.0",
    "next-auth": "^5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "react": "^19",
    "react-dom": "^19",
    "recharts": "^3.6.0",
    "sonner": "^2.0.7",
    "stripe": "^19.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.6",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240903.0",
    "@eslint/eslintrc": "^3.3.3",
    "@opennextjs/cloudflare": "^1.0.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "braces": "^3.0.3",
    "eslint": "^9",
    "eslint-config-next": "^15.1.0",
    "fill-range": "^7.1.1",
    "micromatch": "^4.0.8",
    "postcss": "^8",
    "tailwindcss": "^4.1.17",
    "typescript": "^5",
    "wrangler": "^4.42.2"
  }
}
